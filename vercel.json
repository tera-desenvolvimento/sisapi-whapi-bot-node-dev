{
    "version": 2,
    "name": "sisapi-whapi-bot-node-v2",
    "builds": [
      { "src": "/src/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/src/index.js" }
    ],
    "crons": [
    {
      "path": "/clearFluxes",
      "schedule": "35 9 * * *"
    }
  ]
}